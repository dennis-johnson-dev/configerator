{
  "name": "configerator",
  "version": "0.0.4",
  "description": "Node.js utility module for generating configuration files",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "clean": "rm -rf lib",
    "prepublish": "npm run build",
    "start": "babel-node ./src",
    "test": "ava",
    "test:watch": "ava --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/songawee/configerator.git"
  },
  "author": "songawee",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/songawee/configerator/issues"
  },
  "homepage": "https://github.com/songawee/configerator#readme",
  "dependencies": {
    "yargs": "^6.3.0"
  },
  "ava": {
    "files": [
      "test/*-spec.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "lerna": "2.0.0-beta.30",
    "mkdirp": "^0.5.1"
  }
}
